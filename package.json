{
  "version": "1.4.0-alpha.84",
  "name": "@lilnouns/lilnouns-camp",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "dev": "pnpm run --recursive --parallel --if-present dev",
    "prestart": "pnpm run build",
    "start": "pnpm run --recursive --parallel --if-present start",
    "build": "turbo build --filter='./packages/*'",
    "postbuild": "[ \"$CF_PAGES\" ] && cd apps/nouns-camp && pnpm next-on-pages --disable-worker-minification || true",
    "lint": "turbo lint",
    "format": "pnpm run --recursive --if-present format",
    "test": "pnpm run --recursive --if-present test"
  },
  "resolutions": {
    "@babel/helpers": ">=7.26.10",
    "@babel/runtime": ">=7.26.10",
    "@babel/traverse": ">=7.23.2",
    "@coinbase/wallet-sdk": ">=4.3.0",
    "@vercel/kv": ">=3.0.0",
    "axios": ">=1.6.0",
    "cross-spawn": ">=7.0.5",
    "debug": ">=4.3.1",
    "elliptic": ">=6.6.1",
    "follow-redirects": ">=1.15.6",
    "ip": ">=1.1.9",
    "path-to-regexp": ">=6.3.0",
    "rollup": ">=4.22.4",
    "secp256k1": ">=5.0.1",
    "semver": ">=7.5.2",
    "tar": ">=6.2.1",
    "typescript": ">=5.0.4",
    "undici": ">=6.21.1",
    "vite": ">=5.4.6",
    "ws": ">=7.5.10"
  },
  "devDependencies": {
    "prettier": "3.5.3",
    "turbo": "2.5.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "funding": [
    {
      "type": "custom",
      "url": "https://ud.me/nekofar.crypto"
    }
  ],
  "packageManager": "pnpm@10.0.0"
}
