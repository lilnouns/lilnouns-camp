    "@emotion/react": "11.14.0",
    "@farcaster/core": "0.18.9",
    "@farcaster/miniapp-sdk": "^0.2.1",
    "@farcaster/miniapp-wagmi-connector": "^1.1.0",
    "@lilnounsdao/assets": "0.5.15",
    "@lilnounsdao/contracts": "0.3.5",
