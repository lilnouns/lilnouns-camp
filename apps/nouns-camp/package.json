    "@radix-ui/react-tooltip": "1.2.8",
    "@sentry/nextjs": "10.11.0",
    "@serwist/next": "9.2.1",
    "@serwist/precaching": "9.2.1",
    "@serwist/sw": "9.2.1",
    "@shades/common": "workspace:0.0.0",
